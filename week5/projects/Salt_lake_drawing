

// Create an integer variable called i and store the value 0 in it:
int alta = 100;
int ancho = 100;

size(800,800);
smooth();
translate(width/2,height/2);
strokeWeight(255);
beginShape();
for(int i=0;i<350;i++)
{
  curveVertex((i*2)*sin(i/5.0),(i*2)*cos(i/5.0));
   curveVertex((i*2)*sin(i/4.0),(i*2)*cos(i/4.0));
   curveVertex((i*2)*sin(i/3.0),(i*2)*cos(i/3.0));
   curveVertex((i*2)*sin(i/2.0),(i*2)*cos(i/2.0));
   curveVertex((i*2)*sin(i/1.0),(i*2)*cos(i/1.0));
}
endShape(); 
